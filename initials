#include <cs50.h>
#include <ctype.h>
#include <stdio.h>
#include <string.h>

int main(void)
{
    string name = GetString();
   
    if (name != NULL)
    
    for (int i = 0, n = strlen(name); i < n; i++)
    
    while (name[i] < name(" " + 1));
   
    printf("%c%c", toupper(name[0]), toupper(name[1]));
    {
    printf("\n");
    }

return 0;
}
